{
    "name": "rephlex187/php-binance-api",
    "description": "PHP Binance API wrapper forked from jaggedsoft/php-binance-api.",
    "keywords": ["Binance","cryptocurrency","WebSocket"],
    "license": "MIT",
    "repositories": [ {
            "type": "vcs",
            "url": "https://github.com/rephlex187/php-binance-api"
    }],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ratchet/pawl": "^0.3.0",
        "react/socket": "^1.0 || ^0.8 || ^0.7",
        "ratchet/rfc6455": "^0.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.1"
    },
    "config": {
    	"optimize-autoloader": true,
        "minimum-stability": "stable"
    },
    "autoload": {
        "classmap": [
          "src/php-binance-api.php",
          "src/php-binance-api-rate-limiter.php"
        ]
    }
}
